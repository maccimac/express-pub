extends layout.pug

block content 
  article.grid-container.portfolio-intro
    div 
      h1 Jennifer Macaranas
      p.lead.text-light
        | Full-stack Developer & UX Designer    
  article.grid-container.portfolio-index
      .grid-x.grid-margin-x.small-up-2.medium-up-2.large-up-3
        each proj, i in projects  
          .cell
            a(href='/project/'+i)
              img.thumbnail(src='/public/img/'+proj.img)
              h5=proj.title
      
